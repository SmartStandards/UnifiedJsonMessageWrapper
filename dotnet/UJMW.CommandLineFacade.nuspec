    <file src="dist\bin-net48\UJMW.CommandLineFacade.dll" target="lib/net48" />
    <file src="dist\bin-net48\UJMW.CommandLineFacade.xml" target="lib/net48" />
    <file src="dist\bin-net48\UJMW.CommandLineFacade.pdb" target="lib/net48" />
    <!-- .NET 6.0 -->
    <file src="dist\bin-net6.0\UJMW.CommandLineFacade.dll" target="lib/net6.0" />
