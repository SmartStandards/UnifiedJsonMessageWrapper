    <file src="dist\bin-net6.0\UJMW.CommandLineFacade.pdb" target="lib/net6.0" />
    <!-- .NET 8.0 -->
    <file src="dist\bin-net8.0\UJMW.CommandLineFacade.dll" target="lib/net8.0" />
    <file src="dist\bin-net8.0\UJMW.CommandLineFacade.xml" target="lib/net8.0" />
    <file src="dist\bin-net8.0\UJMW.CommandLineFacade.pdb" target="lib/net8.0" />
