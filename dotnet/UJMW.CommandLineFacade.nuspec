      <group targetFramework=".NETFramework6.0">
        <dependency id="Newtonsoft.Json" version="[13.0.3,13.9999]" />
      </group>
      <group targetFramework=".NETFramework8.0">
        <dependency id="Newtonsoft.Json" version="[13.0.3,13.9999]" />
      </group>
    </dependencies>
  </metadata>
  <files>
